programa {
  funcao inicio() {
    real numero
    escreva("Digite um numero de 01 a 10: ")
    leia(numero)
    se(numero >0 e numero <11){
      escreva("O numero informado foi ", numero, ", ele está entre 01 e 10")
    }
    senao{escreva("O numero informado foi ", numero, " ele não esta entre 01 e 10")
  }
     
  
    
  }
}
